    <PostBuildEvent>
SETLOCAL

set CRC_ADDRESS=0x37FE
set MODULE_NAME=AC

FOR /F "tokens=3" %%G IN ('FINDSTR /C:"#define SOFT_VER_H" "$(MSBuildProjectDirectory)\main_template.c" ') DO SET "SOFT_H=%%G"
FOR /F "tokens=3" %%G IN ('FINDSTR /C:"#define SOFT_VER_L" "$(MSBuildProjectDirectory)\main_template.c" ') DO SET "SOFT_L=%%G"  
FOR /F "tokens=3" %%G IN ('FINDSTR /C:"#define MODULE_VER_H" "$(MSBuildProjectDirectory)\main_template.c" ') DO SET "MODULE_H=%%G"  
FOR /F "tokens=3" %%G IN ('FINDSTR /C:"#define MODULE_VER_L" "$(MSBuildProjectDirectory)\main_template.c" ') DO SET "MODULE_L=%%G"

SET FILE_NAME=%MODULE_NAME%_v%MODULE_H%.%MODULE_L%_s%SOFT_H%.%SOFT_L%

srec_cat "$(OutputDirectory)\$(OutputFileName).hex" -intel -fill 0xFF  0x0000 %CRC_ADDRESS% -crc16-b-e %CRC_ADDRESS% -XMODEM -o  "%FILE_NAME%.bin" -Binary
srec_cat "$(OutputDirectory)\$(OutputFileName).hex" -intel -fill 0xFF  0x0000 %CRC_ADDRESS% -crc16-b-e %CRC_ADDRESS% -XMODEM -o  "%FILE_NAME%_intel.hex" -Intel -Output_Block_Size=16

COPY "$(SolutionDir)..\%MODULE_NAME% BOOT v1\%MODULE_NAME% BOOT v1\Debug\%MODULE_NAME% BOOT v1.hex" "$(OutputDirectory)\%MODULE_NAME% BOOT v1.hex"

srec_cat "$(OutputDirectory)\%MODULE_NAME% BOOT v1.hex" -Intel  "$(OutputDirectory)\%FILE_NAME%_intel.hex" -Intel -o "$(OutputDirectory)\FW_AND_BOOT.hex" -Intel -Output_Block_Size=16
TYPE "$(OutputDirectory)\FW_AND_BOOT.hex" | findstr /v :04000005 &gt;"$(OutputDirectory)\FW_AND_BOOT_final.hex"

ENDLOCAL</PostBuildEvent>
    <PreBuildEvent>
SubWCRev "$(MSBuildProjectDirectory)" "$(MSBuildProjectDirectory)\main_template.c" "$(MSBuildProjectDirectory)\main.c"

DEL /Q "$(OutputDirectory)\*"</PreBuildEvent>
